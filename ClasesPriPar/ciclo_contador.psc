Algoritmo ciclo_contador
	// Un contador es generalmente de tipo ENTERO
	Definir cont Como Entero;
	cont <- 0; // Un contador debe tener un valor inicial
	// Antes de escribir el ciclo
	
	Mientras cont <= 10 Hacer
		Escribir cont;
		cont <- cont + 1;
	Fin Mientras
	
FinAlgoritmo
